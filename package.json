{
  "name": "leo",
  "version": "1.0.0",
  "description": "react_webpack",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --progress --host 127.0.0.1",
    "build": "./node_modules/.bin/webpack --progress --colors --config webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint app",
    "clean": "rm -rf build/*"
  },
  "betterScripts": {
    "dev": {
      "command": "./node_modules/.bin/webpack-dev-server --progress",
      "env": {
        "BUILD_DEV": "development"
      }
    },
    "build": {
      "env": {
        "BUILD_PRERELEASE": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WYoYao/react-webpack-boilerplate.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "leo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WYoYao/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/WYoYao/react-webpack-boilerplate#readme",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "open-browser-webpack-plugin": "0.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "remarkable": "^1.7.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
